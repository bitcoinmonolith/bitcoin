generator client {
    provider = "deno run -A npm:prisma-kysely"
    output   = "./generated"
}

datasource db {
    provider = "sqlite"
    url      = "file:./generated/prototype.sqlite"
}

model Block {
    blockHash       Bytes  @id
    blockHeight     BigInt @unique
    merkleRootCache Bytes?
}

model TxHash {
    shortestUniqueTxHash Bytes  @id
    txId                 BigInt
}
